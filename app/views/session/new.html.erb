<% provide(:title, "Sign in") %>
<h1>Sign in</h1>
<div class="form">
  <%= form_for(:session, url: session_index_path) do |f| %>
  
  <%= f.label :email %>
  <%= f.text_field :email %>
  
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>
<div class="submit">
  <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
  <% end %>
</div>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>
